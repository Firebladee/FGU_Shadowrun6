<root version="2.0">

<!-- first subwindow shows all note items -->
	<windowclass name="charsheet_7_2_1">
		<placement>
			<size>
				<width>252</width>
				<height>611</height>
			</size>
		</placement>
		<sizelimits>
			<maximum>
				<height>100000</height>
			</maximum>
		</sizelimits>
		<nodelete />
		<sheetdata>					
		
	<!-- notes-->
			<genericcontrol name="noteframe">
				<bounds>83,148,515,474</bounds>
				<frame>
					<name>sheetgroup</name>
				</frame>
			</genericcontrol>
			<windowlist name="list_note_1">
				<anchored>
					<to>noteframe</to>
					<position>over</position>
					<offset>-8,-28</offset>
				</anchored>
				<allowcreate />
				<allowdelete />
				<datasource>.notelist</datasource>
				<class>charsheet_noteitem_all</class>
				<script file="charsheet/scripts/charsheet_list_notes.lua" />
			</windowlist>			
			
			<stringcontrol name="labelone">
				<anchored>
					<to>noteframe</to>
					<position>insidetopleft</position>
					<offset>18,12</offset>
					<size>
						<width>65</width>
					</size>					
				</anchored>
				<font>sheetlabelsmall</font>
				<static>Location</static>
			</stringcontrol>			
			<stringcontrol name="labeltwo">
				<anchored>
					<to>noteframe</to>
					<position>insidetopleft</position>
					<offset>100,12</offset>
					<size>
						<width>65</width>
					</size>
				</anchored>
				<font>sheetlabelsmall</font>
				<static>Date</static>
			</stringcontrol>
		</sheetdata>		
	</windowclass>
	
