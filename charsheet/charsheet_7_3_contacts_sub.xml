<root version="2.0">

<!-- first subwindow shows all contact items -->
	<windowclass name="charsheet_7_3_1">
		<placement>
			<size>
				<width>252</width>
				<height>611</height>
			</size>
		</placement>
		<sizelimits>
			<maximum>
				<height>100000</height>
			</maximum>
		</sizelimits>
		<nodelete />
		<sheetdata>					
		
	<!-- contacts-->
			<numberfield name="contactsort" source="contactsort">
				<bounds>0,0,0,0</bounds>
				<script>
					function onValueChanged()
						window.list_contact_1.applySort();
					end
				</script>
				<invisible />
			</numberfield>			
			
			<genericcontrol name="contactframe">
				<bounds>83,148,515,474</bounds>
				<frame>
					<name>sheetgroup</name>
				</frame>
			</genericcontrol>
			<windowlist name="list_contact_1">
				<anchored>
					<to>contactframe</to>
					<position>over</position>
					<offset>-8,-28</offset>
				</anchored>
				<allowcreate />
				<allowdelete />
				<datasource>.contactlist</datasource>
				<class>charsheet_contactitem_all</class>
				<script file="charsheet/scripts/charsheet_list_contact.lua" />
			</windowlist>			
			
			<stringcontrol name="labelone">
				<anchored>
					<to>contactframe</to>
					<position>insidetopleft</position>
					<offset>15,12</offset>
					<size>
						<width>65</width>
					</size>					
				</anchored>
				<font>sheetlabelsmall</font>
				<static>Location</static>
			</stringcontrol>			
			<stringcontrol name="labeltwo">
				<anchored>
					<to>contactframe</to>
					<position>insidetopleft</position>
					<offset>95,12</offset>
					<size>
						<width>65</width>
					</size>
				</anchored>
				<font>sheetlabelsmall</font>
				<static>Name</static>
			</stringcontrol>

			<stringcontrol name="labelthree">
				<anchored>
					<to>contactframe</to>
					<position>insidetopleft</position>
					<offset>390,12</offset>
				</anchored>
				<font>sheetlabelsmall</font>
				<static>CR</static>
				<tooltip>
					<text>connection rating</text>
				</tooltip>
			</stringcontrol>			
			<stringcontrol name="labelfour">
				<anchored>
					<to>contactframe</to>
					<position>insidetopleft</position>
					<offset>440,12</offset>
				</anchored>
				<font>sheetlabelsmall</font>
				<static>LR</static>
				<tooltip>
					<text>loyality rating</text>
				</tooltip>
			</stringcontrol>
		</sheetdata>		
	</windowclass>
	
