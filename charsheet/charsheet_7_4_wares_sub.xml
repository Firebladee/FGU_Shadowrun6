<root version="2.0">

<!-- first subwindow shows all ware items -->
	<windowclass name="charsheet_7_4_1">
		<placement>
			<size>
				<width>252</width>
				<height>611</height>
			</size>
		</placement>
		<sizelimits>
			<maximum>
				<height>100000</height>
			</maximum>
		</sizelimits>
		<nodelete />
		<sheetdata>					
		
	<!-- wares-->
			<numberfield name="waresort" source="waresort">
				<bounds>0,0,0,0</bounds>
				<script>
					function onValueChanged()
						window.list_ware_1.applySort();
					end
				</script>
				<invisible />
			</numberfield>			
			
			<genericcontrol name="wareframe">
				<bounds>83,148,515,474</bounds>
				<frame>
					<name>sheetgroup</name>
				</frame>
			</genericcontrol>
			<windowlist name="list_ware_1">
				<anchored>
					<to>wareframe</to>
					<position>over</position>
					<offset>-8,-28</offset>
				</anchored>
				<allowcreate />
				<allowdelete />
				<datasource>.else.warelist</datasource>
				<class>charsheet_wareitem_all</class>
				<script file="charsheet/scripts/charsheet_list_ware.lua" />
			</windowlist>			
			
			<stringcontrol name="labelone">
				<anchored>
					<to>wareframe</to>
					<position>insidetopleft</position>
					<offset>15,12</offset>
					<size>
						<width>65</width>
					</size>					
				</anchored>
				<font>sheetlabelsmall</font>
				<static>Type</static>
			</stringcontrol>			
			<stringcontrol name="labeltwo">
				<anchored>
					<to>wareframe</to>
					<position>insidetopleft</position>
					<offset>95,12</offset>
					<size>
						<width>65</width>
					</size>
				</anchored>
				<font>sheetlabelsmall</font>
				<static>Name</static>
			</stringcontrol>

			<stringcontrol name="labelthree">
				<anchored>
					<to>wareframe</to>
					<position>insidetopleft</position>
					<offset>400,12</offset>
				</anchored>
				<font>sheetlabelsmall</font>
				<static>Essenceloss</static>
				<tooltip>
					<text>Essenceloss</text>
				</tooltip>
			</stringcontrol>			
		</sheetdata>		
	</windowclass>
	
